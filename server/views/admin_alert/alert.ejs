<% if (locals.view_messages.length!=0) { %>
    <div class="col-12">
        <div class="bg-light rounded h-100 p-4">
            <h6 class="mb-4">Alerts</h6>
            <% locals.view_messages.forEach(alert=> {%>
                <div class="alert alert-<%= alert.status %>" role="alert">
                    <%= alert.message %>
                </div>
                <%}); %>
        </div>
    </div>
<% } %>